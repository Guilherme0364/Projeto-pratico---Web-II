<body>
	<%- include('partials/navbar.ejs') %>
		<div class="container">
			<hr>
			<h1>Perguntas</h1>
			<hr>
			<a href="/question" class="btn btn-primary">Nova pergunta</a>
			<hr>
			<% perguntas.forEach(pergunta=> { %>
				<div class="card">
					<div class="card-body">
						<h3>
							<%= pergunta.titulo %>
						</h3>
					</div>
					<div class="footer">
						<a href="/question/<%= pergunta.id %>" class="btn btn-primary">
							
						</a>
					</div>
				</div>
				<hr>
				<% }) %>
		</div>
		<%- include('partials/footer.ejs') %>
</body>